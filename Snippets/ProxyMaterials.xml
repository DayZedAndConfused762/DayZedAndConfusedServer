<!-- this next part would be adjusted in types file so the planks and sheets spawn full. not needed if you dont want -->



<type name="WoodenPlank">
	<nominal>0</nominal>
	<lifetime>14400</lifetime>
	<restock>0</restock>
	<min>0</min>
	<quantmin>100</quantmin> <!-- change to 100 -->
	<quantmax>100</quantmax> <!-- change to 100 -->
	<cost>100</cost>
	<flags count_in_cargo="0" count_in_hoarder="0" count_in_map="1" count_in_player="0" crafted="1" deloot="0"/>
	<category name="tools"/>
</type>

<type name="MetalPlate">
	<nominal>60</nominal>
	<lifetime>14400</lifetime>
	<restock>0</restock>
	<min>40</min>
	<quantmin>100</quantmin> <!-- change to 100 -->
	<quantmax>100</quantmax> <!-- change to 100 -->
	<cost>100</cost>
	<flags count_in_cargo="0" count_in_hoarder="0" count_in_map="1" count_in_player="0" crafted="0" deloot="0"/>
	<category name="tools"/>
	<tag name="floor"/>
	<usage name="Industrial"/>
</type>

<!-- this next part can be changed if you would like the pallet to spawn where or with the normal pallet of planks spawn
	I have it set up that only the pallet with materials spawns and not the vanilla PileOfWoodenPlanks-->

<event name="ItemPlanks">
	<nominal>50</nominal>
	<min>40</min>
	<max>50</max>
	<lifetime>7200</lifetime>
	<restock>0</restock>
	<saferadius>250</saferadius>
	<distanceradius>20</distanceradius>
	<cleanupradius>100</cleanupradius>
	<flags deletable="0" init_random="0" remove_damaged="0"/>
	<position>fixed</position>
	<limit>child</limit> <!-- change to child-->
	<active>1</active>
	<children>						<!-- put max and min to 1 -->
		<child lootmax="0" lootmin="0" max="1" min="1" type="PileOfWoodenPlanks"/> <!-- you can remove this line, comment it out, or leave it. up to you-->
		<child lootmax="0" lootmin="0" max="1" min="1" type="StaticObj_Misc_Pallet"/> <!-- this line will add the pallet to the plank event -->
	</children>				<!-- if you want it to replace PilleOfWoodenPlanks, just replace the name with Staticobj_Misc_Pallet. -->
</event>


<!-- If you want it as an event for a single spawn, you can use the codes below. just need events and eventspawn files-->
<!-- events-->
<event name="StaticProxyMaterials">
    <nominal>1</nominal>
    <min>1</min>
    <max>1</max>
    <lifetime>600</lifetime>
    <restock>0</restock>
    <saferadius>0</saferadius>
    <distanceradius>0</distanceradius>
    <cleanupradius>10</cleanupradius>
    <flags deletable="1" init_random="0" remove_damaged="1"/>
    <position>fixed</position>
    <limit>child</limit>
    <active>1</active>
    <children>
        <child lootmax="0" lootmin="0" max="1" min="1" type="StaticObj_Misc_Pallet"/>
    </children>
</event>


<!-- eventspawn-->
<event name="StaticProxyMaterials">
    <pos x="0" z="0" a="-1" /> <!-- change the x z and a as needed-->
</event>